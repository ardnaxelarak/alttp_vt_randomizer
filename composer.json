{
  "name": "sporchia/alttp_php_randmizer",
  "description": "PHP based Randomizer for ALttP",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "veetorp",
      "email": "veetor+alttp@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-bcmath": "*",
    "ezyang/htmlpurifier": "^4.9",
    "graham-campbell/markdown": "^14.0",
    "hashids/hashids": "^4.0",
    "laravel/framework": "^9.0",
    "laravel/horizon": "^5.0",
    "laravel/passport": "^10.0",
    "laravel/tinker": "^2.2",
    "laravel/ui": "^4.0",
    "league/flysystem-aws-s3-v3": "^3.0",
    "laraeast/laravel-vue-i18n-generator": "^2.0",
    "sentry/sentry": "^3.7",
    "sentry/sentry-laravel": "^2.0",
    "spatie/laravel-cookie-consent": "^3.2.2",
    "z3/enemizer_linux": "^7.2",
    "z3/enemizer_osx": "^7.2",
    "z3/entrancerandomizer": "^0.6.3",
    "z3/overworldrandomizer": "^1.0.1.3",
    "z3/randomizer": "^31.0",
    "z3/randomizer_ow": "^31.0"
  },
  "require-dev": {
    "spatie/laravel-ignition": "^1.0",
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": "^1.2",
    "mohammedmanssour/form-request-tester": "dev-master",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.0"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "z3/randomizer",
        "version": "31.0.11",
        "source": {
          "url": "https://github.com/ardnaxelarak/z3randomizer",
          "type": "git",
          "reference": "ad44b610fb1ef853044c3109e28b1a84a3f24988"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "z3/randomizer_ow",
        "version": "31.0.10",
        "source": {
          "url": "https://github.com/ardnaxelarak/z3randomizer",
          "type": "git",
          "reference": "015f769521c4ad98f1b3eac2b8dfedb21e8cd592"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "z3/enemizer_linux",
        "version": "7.2.1-alpha",
        "dist": {
          "url": "https://github.com/ardnaxelarak/Enemizer/releases/download/v7.2.1-alpha/ubuntu.16.04-x64.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "z3/enemizer_osx",
        "version": "7.2.1-alpha",
        "dist": {
          "url": "https://github.com/ardnaxelarak/Enemizer/releases/download/v7.2.1-alpha/osx.10.12-x64.zip",
          "type": "tar"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "z3/entrancerandomizer",
        "version": "0.6.3.10",
        "source": {
          "url": "https://github.com/KevinCathcart/ALttPEntranceRandomizer",
          "type": "git",
          "reference": "tags/0.6.3-pre10"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "z3/overworldrandomizer",
        "version": "1.0.1.3",
        "source": {
          "url": "https://github.com/ardnaxelarak/ALttPDoorRandomizer",
          "type": "git",
          "reference": "c051b6e9dc68058a5f3b0945c9e26629e95786a3"
        }
      }
    }
  ],
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "ALttP\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "test": [
      "phpunit --colors=always --coverage-clover=./report/coverage.clover --coverage-html=./report"
    ],
    "coverage": [
      "Composer\\Config::disableProcessTimeout",
      "phpdbg -qrr ./vendor/bin/phpunit --colors=always --coverage-clover=./report/php.coverage.clover --coverage-html=./coverage/php"
    ],
    "cloc": [
      "cloc --exclude-dir=node_modules,vendor,report,storage,bin,public --not-match-f=\"(lock|generated)\" ."
    ]
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
